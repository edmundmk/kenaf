def isprime( n )
    for i = 2 : n - 1 do
        if n % i == 0 then return false end
    end
    return true
end

def primes( n )
    var count = 0
    for i = 2 : n do
        if isprime( i ) then count += 1 end
    end
    return count
end

print( "primes: " ~ string( primes( 200000 ) ) )

